{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "start frontend",
        "type": "shell",
        "command": "npm",
        "args": ["start"],
        "isBackground": true,
        "problemMatcher": {
          "owner": "external",
          "pattern": {
            "regexp": "^(.*)$",
            "file": 1,
            "location": 2,
            "message": 3
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "Compiling...",
            "endsPattern": "Compiled successfully."
          }
        }
      }
    ]
  }
  